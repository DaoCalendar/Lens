<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="oval">
            <corners android:radius="5dp"
                />
            <solid android:color="#ffffff"
                />

            <stroke android:color="#000000"
                android:width="3px"
                />
        </shape>
    </item>
    <item
        android:left="3dp"
        android:right="3dp">
        <shape
            android:shape="line">
            <stroke
                android:width="2dp"
                android:color="#91000000" />
        </shape>
    </item>
    <item
        android:left="3dp"
        android:right="3dp">
        <rotate
            android:fromDegrees="90"
            android:toDegrees="90"
            android:pivotX="50%"
            android:pivotY="50%">
            <shape
                android:shape="line">
                <stroke
                    android:width="2dp"
                    android:color="#91000000" />
            </shape>
        </rotate>
    </item>
</layer-list>