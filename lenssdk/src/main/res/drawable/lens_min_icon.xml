<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="7dp"
        android:right="7dp"
        android:top="7dp"
        android:bottom="7dp"
        >
        <shape
            android:shape="oval">
            <corners android:radius="5dp"
                />

            <solid android:color="#fcb42e"
                />

            <stroke android:color="#c6b285"
                android:width="1px"
                />
        </shape>
    </item>
    <item
        android:left="10dp"
        android:right="10dp">
        <shape
            android:shape="line">
            <stroke
                android:width="1.5dp"
                android:color="#91000000" />
        </shape>
    </item>
</layer-list>